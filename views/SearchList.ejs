<%- include("header") -%>

 <div class="container-fluid">
  <div class="row">
    <div class="col-md-4"></div>
   <div class="col-md-6 col-sm-6 col-xs-12">
    <form method="post">
      <div class="form-group"> <!-- Date input -->
        <label class="control-label"><h2>Please fill the details</h2></label>
        <input class="form-control bootstrap-iso" id="date" name="date" placeholder="MM/DD/YYY" type="text" autocomplete="off" />
      </div>
      <div class="form-group">
        <input class="form-control" name="username" placeholder="Enter Email" type="text" autocomplete="off" />
      </div>
      <div class="form-group"> <!-- Submit button -->
        <button class="btn btn-primary " name="submit" type="submit">Submit</button>
      </div>
     </form>

  </div>    
 </div>
</div>
<%- include("footer") -%>